			<div class="apb_product_detail_add_to_cart_container_sold_out sold_out_{{sectionId}}" {% if
				p.selected_or_first_available_variant.available %}hidden{% endif %}>
				<button class="apb_product_detail_add_to_cart_sold_out" disabled>
					<span class="apb_product_detail_add_to_cart_text_sold_out">
						{{ 'products.product.sold_out' | t }}</span>
				</button>
			</div>

			{%endif%}

			{% else %}
			<!-- Multiple variant - add to cart button -->
			<button class="apb_product_list_product_item_btn" data-submit-button id="AddToCart-{{ section.id }}"
				data-product-id="{{ p.external_product_id }}" data-checkout="{{ buttonAction }}"
				ga-product-id="{{ p.id }}" ga-product-name="{{ p.title }}"
				ga-button-type="{% if buttonType == 'addCartButton' %}addCartButton{% else %}checkoutButton{% endif %}">
				{{ action_cart_button_text }}
			</button>
			<!-- select variant modal -->
			<div class="product_modal product_modal_hidden" data-section-id="{{sectionId}}" ga-product-id="{{ p.id }}"
				ga-product-name="{{ p.title }}"
				ga-button-type="{% if buttonType == 'addCartButton' %}addCartButton{% else %}checkoutButton{% endif %}">
				<div class="apb_wrapper apb_wrapper_animation_hide">
					<div class="apb_info_wrapper">
						<div class="apb_cancel" tabindex="0" type="button" aria-label="close">
							<span class="648-MuiIconButton-label">
								<svg viewBox="0 0 20 20" class="Polaris-Icon__Svg" focusable="false" aria-hidden="true">
									<path
										d="M11.414 10l6.293-6.293a1 1 0 1 0-1.414-1.414L10 8.586 3.707 2.293a1 1 0 0 0-1.414 1.414L8.586 10l-6.293 6.293a1 1 0 1 0 1.414 1.414L10 11.414l6.293 6.293A.998.998 0 0 0 18 17a.999.999 0 0 0-.293-.707L11.414 10z">
									</path>
								</svg>
							</span>
						</div>
						{% if show_product_name%}
						<div class="apb_title_wrapper">
							<div class="apb_product_detail_information_title">
								{{p.title}}
							</div>
						</div>
						{% endif%}
						<div class="apb_product_detail_price_container">
							{% if show_product_price and p.selected_or_first_available_variant.price != 0 %}
							<span class="apb_product_detail_price" style=color:{{salePriceColor}}
								id="price_{{sectionId}}">
								{% if p.selected_or_first_available_variant.price %}
								{% assign render_price = p.selected_or_first_available_variant.price %}
								{% else %}
								{% assign render_price = p.first_available_variant.price %}
								{% endif %}

								{% if settings.currency_code_enabled %}
								{{ render_price | money_with_currency }}
								{% else %}
								{{ render_price | money }}
								{% endif %}
							</span>
							{% endif %}
							{% if show_compare_at_price and p.selected_or_first_available_variant.compare_at_price != 0
							%}
							<span class="apb_product_detail_compare_price" style=color:{{comparePriceColor}}
								id="compare_at_price_{{sectionId}}">
								{% if p.selected_or_first_available_variant.compare_at_price %}
								{% assign render_compare_at_price =
								p.selected_or_first_available_variant.compare_at_price
								%}
								{% else %}
								{% assign render_compare_at_price = p.first_available_variant.compare_at_price %}

								{% endif %}

								{% if settings.currency_code_enabled %}
								{{ render_compare_at_price | money_with_currency }}
								{% else %}
								{{ render_compare_at_price | money }}
								{% endif %}
							</span>
							{% endif %}
						</div>
					</div>
					{% capture "form_id" %}AddToCartForm-{{ section.id }}{%- endcapture %}
					<div class="apb_scrollable_area">
						<form method="post" value="{{ p.first_available_variant.id }}" class="apb-product-form"
							section-type='productList' data-product-id="{{ p.external_product_id }}"
							data-section-id="{{ sectionId }}" autocomplete="off">
							<input name="form_type" value='product' type="hidden">
							<input name="utf8" value='✓' type="hidden">
							<input name="id" value="{{ p.first_available_variant.id }}" type="hidden"
								apb_variant_input="{{sectionId}}">
							<input name="quantity" value=1 type="hidden">

							<div class="apb_variants">
								<div class="apb_product_detail_options_container">
									<div class="apb_product_detail_options_group">
										{% for option in p.options_with_values %}
										{{option.value}}
										<div class="apb_product_detail_option_container 
            										        {% if variantType == 'dropdown' %}
            										            drop_down_options
            										        {% else %}
            										            button_options
            										        {% endif %}">
											<div class="apb_product_detail_option_description">
												<span>
													{{ option.name }}
												</span>
											</div>
											{% if variantType == 'dropdown' %}
											<select class="apb_product_detail_option" data-handle="{{p.handle}}">
												{% for value in option.values %}
												<option class="options_value_item" value="{{ value | escape }}" {% if
													option.selected_value == value %} selected {% endif %}
													data-product-id="{{ p.external_product_id }}"
													data-section-id="{{ sectionId }}">
													{{ value }}
												</option>
												{% endfor %}
											</select>
											{% else %}
											{% for value in option.values %}
											<label class="apb_product_detail_button_variant
            										                {% if option.selected_value == value %} 
            										                    active
            										                {% endif %}
            										            ">
												<input class="options_value_item button_variant_input" type="radio"
													value="{{value | escape}}" {% if option.selected_value == value %}
													checked {% endif %} data-handle="{{p.handle}}"
													data-product-id="{{ p.external_product_id }}"
													data-section-id="{{sectionId}}" name="{{option.name}}"
													apb_variant_input="{{sectionId}}">
												{{value}}
											</label>
											{% endfor %}
											{% endif %}
										</div>

										{% endfor %}

									</div>
								</div>
							</div>

							{% if buttonType == 'addCartButton' %}
							<div class="apb_product_detail_add_to_cart_container sold_not_out_{{sectionId}}" 
							{% if p.first_available_variant.id == null or p.first_available_variant.id == undefined or
								p.selected_variant.available == false %}hidden{% endif %}>
								<button type="submit" class="apb_product_detail_action_btn apb_product_detail_add_to_cart 
									{% if actionLength == 1 %} apb_product_detail_first_btn {% else %} apb_product_detail_second_btn {% endif %}"
									name="add" data-submit-button id="AddToCart-{{ section.id }}"
									data-product-id="{{ p.external_product_id }}" data-checkout="{{ buttonAction }}">
									<div class="apb_product_detail_add_to_cart_loading">
										<div hidden></div>
									</div>
									<div class="apb_buy_group">
										<div class="apb_product_detail_add_to_cart_text"
											id="AddToCartText-{{ section.id }}">{{action_cart_button_text}}
										</div>
									</div>
								</button>
							</div>
							{% endif %}
							<div class="apb_product_detail_add_to_cart_container_sold_out sold_out_{{sectionId}}" {% if
								p.selected_or_first_available_variant.available %}hidden{% endif %}>
								<button class="apb_product_detail_add_to_cart_sold_out" disabled>
									<span class="apb_product_detail_add_to_cart_text_sold_out">
										{{ 'products.product.sold_out' | t }}</span>
								</button>

							</div>

							<div class="apb_product_detail_add_to_cart_state apb_product_detail_add_to_cart_success">
								<img class="apb_customer_form_banner_icon"
									src="https://websites.am-static.com/automizely/images/pages/statics/tick.svg"
									alt="success_icon" />
								<span>Added to cart</span>
							</div>

							<div
								class="apb_product_detail_add_to_cart_state apb_product_detail_add_to_cart_failed fail_nomore">
								<img class="apb_customer_form_banner_icon"
									src="https://websites.am-static.com/automizely/images/pages/statics/warning_round_shape.svg"
									alt="success_icon" />
								<span>All {number} {{p.title}} - {variant} already in your cart</span>
							</div>
							<div
								class="apb_product_detail_add_to_cart_state apb_product_detail_add_to_cart_failed fail_network">
								<img class="apb_customer_form_banner_icon"
									src="https://websites.am-static.com/automizely/images/pages/statics/warning_round_shape.svg"
									alt="success_icon" />
								<span>Network error. Please try again</span>
							</div>
						</form>
						{% if buttonType == 'checkoutButton' and p.first_available_variant.id != null and
						p.first_available_variant.id != undefined %}
						<div class="apb_buy_now_box_{{sectionId}} {% if actionLength != 1 %}
									margin-t {% endif %}" {% if p.selected_variant.available == false %}hidden{% endif %}>
							{% form 'product', p %}
							<input class="liquid_by_now_form_{{sectionId}}" name="id"
								value="{{ p.first_available_variant.id }}" disabled style="display:none">
							{{ form | payment_button }}
							{% endform %}
						</div>
						{% endif %}
					</div>
				</div>
			</div>
			{% endif %}
		</div>
		{% else %}
		{% endif %}
		{% else %}
		<div class="apb_product_list_product_item_btn_soldout">
			Sold out
		</div>
		{% endif %}
		{% endif %}
	</div>
</div>
{% endif %}

{% endfor %}
<!-- sample products -->
{% else %}
{{ sample_product_content }}
{% endif %}
</div></div></div></div></div></div><div>{% if article.tags.size > 0 %}
<div class="apb_tags_container">
	{% for tag in article.tags %}
	<a class="apb_tag" href="{{ blog.url }}/tagged/{{ tag | handle }}">{{
		tag
	}}</a>
	{% endfor %}
</div>
{% endif %}</div><script src="https://s3.amazonaws.com/websites.am-static.com/automizely/productDetailAddToCart/productDetailAddToCart_v13.js" async=""></script><script src="https://widgets.automizely.com/pages/v1/uiLogic.js?store_connection_id=f8b19ada100040e9b42d5ce99227f588"></script></div>
	